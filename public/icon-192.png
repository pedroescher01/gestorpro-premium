<?xml version="1.0" encoding="UTF-8"?>
<svg width="192" height="192" viewBox="0 0 192 192" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background com gradiente -->
  <rect width="192" height="192" rx="32" fill="url(#gradient)"/>
  
  <!-- Definição do gradiente -->
  <defs>
    <linearGradient id="gradient" x1="0" y1="0" x2="192" y2="192" gradientUnits="userSpaceOnUse">
      <stop offset="0%" stop-color="#0D0D0D"/>
      <stop offset="100%" stop-color="#1A1A1A"/>
    </linearGradient>
  </defs>
  
  <!-- Ícone ERP - Gráfico de barras estilizado -->
  <g transform="translate(48, 48)">
    <!-- Barra 1 -->
    <rect x="0" y="40" width="20" height="56" rx="4" fill="#00E5FF" opacity="0.8"/>
    <!-- Barra 2 -->
    <rect x="28" y="20" width="20" height="76" rx="4" fill="#00E5FF"/>
    <!-- Barra 3 -->
    <rect x="56" y="30" width="20" height="66" rx="4" fill="#00E5FF" opacity="0.9"/>
    <!-- Barra 4 -->
    <rect x="84" y="10" width="20" height="86" rx="4" fill="#00E5FF" opacity="0.7"/>
    
    <!-- Linha de tendência -->
    <path d="M 10 50 L 38 30 L 66 35 L 94 15" stroke="#00E5FF" stroke-width="3" stroke-linecap="round" stroke-linejoin="round" fill="none" opacity="0.5"/>
    
    <!-- Pontos na linha -->
    <circle cx="10" cy="50" r="4" fill="#00E5FF"/>
    <circle cx="38" cy="30" r="4" fill="#00E5FF"/>
    <circle cx="66" cy="35" r="4" fill="#00E5FF"/>
    <circle cx="94" cy="15" r="4" fill="#00E5FF"/>
  </g>
  
  <!-- Texto GestorPro -->
  <text x="96" y="155" font-family="Inter, sans-serif" font-size="18" font-weight="bold" fill="#00E5FF" text-anchor="middle">GestorPro</text>
</svg>
