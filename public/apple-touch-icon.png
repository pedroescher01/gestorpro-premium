<?xml version="1.0" encoding="UTF-8"?>
<svg width="180" height="180" viewBox="0 0 180 180" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background com gradiente -->
  <rect width="180" height="180" rx="40" fill="url(#gradient)"/>
  
  <!-- Definição do gradiente -->
  <defs>
    <linearGradient id="gradient" x1="0" y1="0" x2="180" y2="180" gradientUnits="userSpaceOnUse">
      <stop offset="0%" stop-color="#0D0D0D"/>
      <stop offset="100%" stop-color="#1A1A1A"/>
    </linearGradient>
  </defs>
  
  <!-- Ícone ERP - Gráfico de barras estilizado -->
  <g transform="translate(45, 45)">
    <!-- Barra 1 -->
    <rect x="0" y="38" width="18" height="52" rx="4" fill="#00E5FF" opacity="0.8"/>
    <!-- Barra 2 -->
    <rect x="26" y="18" width="18" height="72" rx="4" fill="#00E5FF"/>
    <!-- Barra 3 -->
    <rect x="52" y="28" width="18" height="62" rx="4" fill="#00E5FF" opacity="0.9"/>
    <!-- Barra 4 -->
    <rect x="78" y="8" width="18" height="82" rx="4" fill="#00E5FF" opacity="0.7"/>
    
    <!-- Linha de tendência -->
    <path d="M 9 48 L 35 28 L 61 33 L 87 13" stroke="#00E5FF" stroke-width="3" stroke-linecap="round" stroke-linejoin="round" fill="none" opacity="0.5"/>
    
    <!-- Pontos na linha -->
    <circle cx="9" cy="48" r="4" fill="#00E5FF"/>
    <circle cx="35" cy="28" r="4" fill="#00E5FF"/>
    <circle cx="61" cy="33" r="4" fill="#00E5FF"/>
    <circle cx="87" cy="13" r="4" fill="#00E5FF"/>
  </g>
  
  <!-- Texto GestorPro -->
  <text x="90" y="145" font-family="Inter, sans-serif" font-size="16" font-weight="bold" fill="#00E5FF" text-anchor="middle">GestorPro</text>
</svg>
